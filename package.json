{
  "name": "agence-test-app",
  "version": "1.0.0",
  "description": "MySQL, Express, React, Node Test Application",
  "scripts": {
    "start": "babel-node server/app.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build": "rimraf dist && npm run webpack:prod && npm run start",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bikranshu/express-react-boilerplate.git"
  },
  "keywords": [
    "node",
    "es6",
    "express",
    "react",
    "redux",
    "react-router",
    "webpack",
    "boilerplate"
  ],
  "author": "Diógenes Hernández <diogenes.hern@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.14.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connected-react-router": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^5.0.0-alpha.7",
    "helmet": "^3.18.0",
    "history": "^4.9.0",
    "http-status-codes": "^1.3.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.16.5",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-material-iconic-font": "^1.2.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "recharts": "^1.7.1",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^3.2.9",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "1.14.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.19.0",
    "react-hot-loader": "^4.8.7",
    "rimraf": "^2.6.3",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "engines": {
    "node": ">= 10.16.0",
    "npm": ">= 6.9.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "extends": [
      "airbnb",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:testcafe/recommended",
      "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "indent": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "max-len": 0,
      "no-alert": 0,
      "no-console": 0,
      "no-restricted-globals": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "class-methods-use-this": 0,
      "prettier/prettier": 2,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "rangeStart": 0,
    "rangeEnd": "Infinity",
    "parser": "babylon",
    "proseWrap": "preserve"
  }
}
